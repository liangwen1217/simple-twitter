<% if tweet.is_liked?(current_user) %>
   <%= link_to "unlike", unlike_tweet_path(tweet), method: :post,class: "links links-red" %>
<% else %>
  <div id="tweet-id-<%= tweet.id %>" class="inline">
    <%= link_to "likes(#{tweet.likes_count}", like_tweet_path(tweet), method: :post, class: "links links-red" %>
  </div>
<% end %>